export class Product {
    id: number;
    title: string;
    price: number;
    images: string[];
    description: string;
    Stock: number;

    constructor(id: number, title: string, price: number, images: string[], description: string, Stock: number){
        this.id = id;
        this.title = title;
        this.price = price;
        this.images = images;
        this.description = description;
        this.Stock = Stock;
    }
}
